{
  "name": "classapijs",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "scripts": {
    "start": "npm run build",
    "watch:dev": "nodemon",
    "build": "npm-run-all clean transpile",
    "clean": "rimraf lib",
    "dev": "NODE_ENV=development npm-run-all build server",
    "prod": "NODE_ENV=production npm-run-all build server",
    "server": "node ./lib/app.js",
    "transpile": "babel -d ./lib ./app.js; babel -d ./lib/src ./src"
  },
  "nodemonConfig": {
    "exec": "npm run dev",
    "watch": [
      "src/config/*",
      "src/modules/*"
    ],
    "ignore": [
      "**/__tests__/**",
      "*.test.js",
      "*spec.js"
    ]
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "morgan": "^1.10.0",
    "winston": "^3.3.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  },
  "dependencies": {
    "@babel/cli": "^7.16.0",
    "nodemon": "^2.0.15",
    "npm-run-all": "^4.1.5",
    "rimraf": "^3.0.2"
  }
}
